<h1 class="page-header">Your apps</h1>

<div class="table-responsive">
  <table class="table table-striped table-hover">
    <thead>
      <tr>
        <th>Name</th>
        <th></th>
      </tr>
    </thead>
    <tbody>
      <% @output.each do |app| %>
        <tr>
          <td><%= app %></td>
          <td><%= link_to fa_icon("remove"), "#", class: "btn btn-sm btn-danger" %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
